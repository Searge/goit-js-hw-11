{
  "name": "goit-js-hw-11",
  "version": "1.1.0",
  "description": "Images search, get with axios from pixabay and render dynamic gallery and lightbox",
  "main": "index.js",
  "homepage": "https://searge.github.io/goit-js-hw-11",
  "scripts": {
    "dev": "parcel src/*.html",
    "build": "parcel build src/*.html --public-url /goit-js-hw-11/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/searge/goit-js-hw-11.git"
  },
  "keywords": [],
  "author": "Alexander Repeta <alexander.repeta@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goitacademy/parcel-project-template/issues"
  },
  "devDependencies": {
    "browserslist": "^4.18.1",
    "caniuse-lite": "^1.0.30001282",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-custom-dist-structure": "^1.1.18",
    "parcel-plugin-handlebars-precompile": "^1.0.2",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "posthtml-include": "^1.6.2",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "notiflix": "^3.2.2",
    "simplelightbox": "^2.10.1"
  },
  "customDistStructure": {
    "config": {
      "js": [
        ".js",
        ".js.map"
      ],
      "css": [
        ".css",
        ".css.map"
      ],
      "fonts": [
        ".otf"
      ],
      "images/photos": [
        ".jpg",
        ".png"
      ],
      "images/svg": [
        ".svg"
      ]
    },
    "options": {
      "development": true
    }
  }
}